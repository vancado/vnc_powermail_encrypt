{
    "name": "vancado/vnc-powermail-encrypt",
    "type": "typo3-cms-extension",
    "description": "Adds encryption to powermail receiver emails with Secure Email (S/MIME) certificate. Supports certificates in .pem format.",
    "authors": [],
    "license": [
        "GPL-2.0-or-later"
    ],
    "require": {
        "typo3/cms-core": "^9.5.20 || ^10.4.6",
        "in2code/powermail": "^7 || ^8"
    },
    "autoload": {
        "psr-4": {
            "Vancado\\VncPowermailEncrypt\\": "Classes"
        }
    },    
    "replace": {
        "vancado/vnc-powermail-encrypt": "self.version",
        "typo3-ter/vnc-powermail-encrypt": "self.version"
    }
}